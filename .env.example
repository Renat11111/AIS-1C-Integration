# Server Configuration
SERVER_PORT=:8081

# Security
# Token for incoming requests from AIS to this Proxy
AIS_TOKEN=change_me_to_secure_token

# 1C Integration Configuration
# Base URL for the 1C HTTP Service
ONEC_BASE_URL=http://localhost/base/hs/ais/v1/data
# Basic Auth for 1C (Leave empty if not used)
ONEC_USER=
ONEC_PASSWORD=

# Performance Tuning
# Number of concurrent workers sending data to 1C
WORKER_COUNT=5
# Number of records fetched from DB in one dispatcher tick
BATCH_SIZE=50
